project AdaPcre is
   for Library_Name     use "adapcre";
   for Library_Kind     use "dynamic";
   for Source_Dirs      use ("/usr/local/include" & "/adapcre");
   for Library_Dir      use "/usr/local/lib";
   for Library_ALI_Dir  use "/usr/local/lib" & "/adapcre";
   for Externally_Built use "true";
   package Linker is
      for Linker_Options use ("-lgnat", "-lgnarl");
   end Linker;
end AdaPcre;

